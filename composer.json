{
    "name": "painlesscode/breeze-multiauth",
    "description": "Multi authentication system with laravel breeze",
    "keywords": ["laravel", "auth", "breeze", "multiauth"],
    "license": "MIT",
    "authors": [
        {
            "name": "Hazzaz Bin Faiz",
            "email": "hazzazbinfaiz@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php" : "^7.3|^8.0",
        "illuminate/filesystem": "^8.42",
        "illuminate/support": "^8.42",
        "illuminate/validation": "^8.42",
        "nikic/php-parser": "^4.6"
    },
    "autoload": {
        "psr-4": {
            "Painless\\BreezeMultiAuth\\" : "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Painless\\BreezeMultiAuth\\BreezeMultiAuthServiceProvider"
            ]
        }
    }
}
